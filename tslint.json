{
	"extends": [
		"tslint:latest"
	],
	"rules": {
		"quotemark": [
			"single"
		],
		"indent": [
			"tabs"
		],
		"forin": false,
		"arrow-parens": [
			false
		],
		"object-literal-sort-keys": false,
		"max-line-length": [
			150
		],
		"no-unused-variable": true,
		"interface-name": [
			false
		],
		"no-console": [
			"log"
		],
		"variable-name": false
	}
}
