{
	"extends": [
		"tslint:latest"
	],
	"rules": {
		"quotemark": [
			true,
			"single"
		],
		"indent": [
			true,
			"tabs"
		],
		"forin": false,
		"arrow-parens": [
			false
		],
		"object-literal-sort-keys": false,
		"max-line-length": [
			true,
			150
		],
		"interface-name": [
			false
		],
		"no-console": [
			true
		],
		"variable-name": false,
		"no-bitwise": false,
		"space-within-parens": [
			false
    ],
    "no-unused-expression": false
	}
}
