    "typescript": "^3.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1"
  },
  "devDependencies": {},
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
