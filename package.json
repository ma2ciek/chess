{
  "name": "chess",
  "version": "0.0.1",
  "scripts": {
    "test": "./node_modules/.bin/mocha --require ts-node/register 'test/**/*.ts'",
    "coverage": "./node_modules/.bin/nyc ./node_modules/.bin/mocha --require ts-node/register 'test/**/*.ts'; ./node_modules/.bin/nyc report --reporter text",
    "test:watch": "./node_modules/.bin/mocha --require ts-node/register 'test/**/*.ts' --watch",
    "start": "./node_modules/.bin/webpack-cli --watch --mode development & node server.js",
    "production": "./node_modules/.bin/webpack-cli --watch --mode production & node server.js"
  },
  "engines": {
    "npm": ">4.6.0",
    "node": ">6.10.0"
  },
  "dependencies": {
    "@types/chai": "^4.1.4",
    "@types/lodash": "^4.14.116",
    "@types/mocha": "^5.2.5",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/sinon": "^5.0.1",
    "chai": "^4.1.2",
    "cloc": "^2.3.3",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.2",
    "istanbul": "^0.4.5",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.0",
    "nyc": "^13.0.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "sass-loader": "^7.0.1",
    "sinon": "^6.1.5",
    "style-loader": "^0.22.0",
    "ts-loader": "^4.5.0",
